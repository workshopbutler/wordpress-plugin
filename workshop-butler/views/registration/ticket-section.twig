<section class="wsb-ticket-section">
    <div class="wsb-form__section-title">{{ wsb_t("form.section.#{section.id}") }}</div>
    {% for field in section.fields %}
        {{ include('field.twig') }}
    {% endfor %}
    {% if false %}
        <div class="wsb-form__field ticket">
            <label class="wsb-label">{{ wsb_t('form.summary.caption') }}</label>
            <div class="wsb-form__summary" data-summary>
            </div>
        </div>
    {% endif %}
</section>
