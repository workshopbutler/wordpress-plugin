{% if event %}
    <a href="{% if registration == 'true' %} {{ event.registration_page.url }} {% else %} {{ event.get_url() }} {% endif %}"
       target="{{ target }}" class="wsb-next-element-button">
        {% if title %} {{ title }} {% else %} {{ wsb_t('nextEvent.register') }} {% endif %}
    </a>
{% else %}
    <span class="wsb-next-element-no-event">
        {% if no_event_title %} {{ no_event_title }} {% else %} {{ wsb_t('nextEvent.notAvailable') }} {% endif %}
    </span>
{% endif %}
