{
    "name": "workshopbutler/wordpress-plugin",
    "description": " This plugin integrates Workshop Butler Events, Trainers and Testimonials to your WordPress website.",
    "homepage": "https://workshopbutler.com/",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.0"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WorkshopButler\\Hooks\\": "workshop-butler/public/includes/hooks"
        }
    }
}
